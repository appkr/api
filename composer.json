{
  "name": "appkr/api",
  "description": "RESTful HTTP API dev tool for Laravel or Lumen based project",
  "keywords": [
    "laravel",
    "restful api",
    "fractal"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "appkr",
      "email": "juwonkim@me.com"
    }
  ],
  "require": {
    "league/fractal": "0.16.*",
    "shrikeh/teapot": "2.3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "6.1.*"
  },
  "autoload": {
    "psr-4": {
      "Appkr\\Api\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "laravel": {
      "providers": [
        "\\Appkr\\Api\\ApiServiceProvider"
      ]
    }
  }
}
